<h1>Registrar Usuario</h1>
  <div class="form-container">
    <form action="api/session/register" method="post" class="form-register">
      <label>Nombre:</label> <input type="text" name="first_name"><br/>
      <label>Apellido:</label> <input type="text" name="last_name"><br/>
      <label>Email:</label> <input type="text" name="email"><br/>
      <label>Password:</label> <input type="password" name="password"><br/>
      <label>Rol:</label>
      <select id="rol" name="rol" style="width: 40%;">
        <option value="Usuario">Usuario</option>
        <option value="Premium">Premium</option>
      </select><br/>      
      <button type="submit">Registrarse</button>
    </form>
  </div>
</form>